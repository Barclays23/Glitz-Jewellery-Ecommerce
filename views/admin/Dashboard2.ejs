    <div class="row sales-report" id="sales-report">
        <div class="col-12 grid-margin">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title text-success"> SALES REPORT </h4>
                    <!-- Link to download PDF -->
                    <a href="/admin/sales-report-pdf" class="btn btn-primary mb-4">
                        <i class="mdi mdi-download"></i> Download PDF Report
                    </a>
                    <div class="table-responsive">
                        <table class="table">
                            <thead>
                                <tr style="text-align: center;">
                                    <th> NO </th>
                                    <th> ORDER NO </th>
                                    <th> ORDER DATE </th>
                                    <th> CUSTOMER </th>
                                    <th> TOTAL AMOUNT </th>
                                    <th> OFFER DISCOUNT </th>
                                    <th> COUPON DISCOUNT </th>
                                    <th> SPECIAL DISCOUNT </th>
                                    <th> NET AMOUNT </th>
                                    <th> PAYMENT MODE </th>
                                    <th> ORDER STATUS </th>
                                </tr>
                            </thead>

                            <tbody>
                                <% if (orderData.length > 0){ %>
                                    <% for (i=0; i< orderData.length; i++){ %>
                                        <tr>
                                            <!-- sl no -->
                                            <td> <%= (currentPage - 1) * limit + i + 1 %></td>

                                            <!-- order no -->
                                            <td> <%= orderData[i].orderNo %> </td>

                                            <!-- order date -->
                                            <td> <%= orderData[i].orderDate.toLocaleDateString('en-GB') %> </td>

                                            <!-- customer -->
                                            <td> <%= orderData[i].userRef.email %> </td>

                                            <!-- subTotal / total amount -->
                                            <td style="text-align: right; color: aquamarine;"> <%= orderData[i].subTotal.toFixed(2) %> </td>

                                            <!-- offerDiscount -->
                                            <%  let orderOfferDiscount = 0;
                                                for (item of orderData[i].orderedItems){
                                                    let itemOfferDiscount = item.offerDiscount ? item.offerDiscount : 0;
                                                    let itemQuantity = item.quantity;
                                                    orderOfferDiscount += itemOfferDiscount * itemQuantity;
                                                } %>
                                            <% if (orderOfferDiscount > 0 ){ %>
                                                <td style="text-align: right; color: yellow;"> <%= orderOfferDiscount.toFixed(2) %> </td>
                                            <% } else { %>
                                                <td style="text-align: right;"> <%= orderOfferDiscount.toFixed(2) %> </td>
                                            <% } %>

                                            <!-- couponDiscount -->
                                            <% const couponDiscount = orderData[i].couponDiscount ? orderData[i].couponDiscount : 0 %>
                                            <% if (couponDiscount > 0 ){ %>
                                                <td style="text-align: right; color: yellow;"> <%= (orderData[i].couponDiscount ? orderData[i].couponDiscount : 0).toFixed(2) %> </td>
                                            <% } else { %>
                                                <td style="text-align: right;"> <%= (orderData[i].couponDiscount ? orderData[i].couponDiscount : 0).toFixed(2) %> </td>
                                            <% } %>

                                            <!-- specialDiscount -->
                                            <% const specialDiscount = orderData[i].specialDiscount ? orderData[i].specialDiscount : 0 %>
                                            <% if (specialDiscount > 0 ){ %>
                                                <td style="text-align: right; color: yellow;"> <%= (orderData[i].specialDiscount ? orderData[i].specialDiscount : 0).toFixed(2) %> </td>
                                            <% } else { %>
                                                <td style="text-align: right;"> <%= (orderData[i].specialDiscount ? orderData[i].specialDiscount : 0).toFixed(2) %> </td>
                                            <% } %>

                                            <!-- netAmount -->
                                            <td style="text-align: right; font-size: 16px; color: rgb(99, 255, 96);"> <b> <%= orderData[i].netAmount.toFixed(2) %> </b> </td>

                                            <!-- paymentMethod -->
                                            <% if (orderData[i].paymentMethod === 'Cash On Delivery' ){ %>
                                                <td class="text-warning" style="text-align: center;"> <%= orderData[i].paymentMethod %> </td>
                                            <% } else if (orderData[i].paymentMethod === 'Wallet' ){ %>
                                                <td class="text-primary" style="text-align: center;"> <%= orderData[i].paymentMethod %> </td>
                                            <% } else if (orderData[i].paymentMethod === 'Online Payment' ){ %>
                                                <td class="text-success" style="text-align: center;"> <%= orderData[i].paymentMethod %> </td>
                                            <% } %>

                                            <!-- orderStatus -->
                                            <% if (orderData[i].orderStatus === 'Pending' ){ %>
                                                <td class="text-center"> <div class="text-center badge badge-outline-warning"> <%= orderData[i].orderStatus %> </div></td>
                                            <% } else if (orderData[i].orderStatus === 'Failed' ){ %>
                                                <td class="text-center"> <div class="text-center badge badge-outline-danger"> <%= orderData[i].orderStatus %> </div></td>
                                            <% } else if (orderData[i].orderStatus === 'Processing' ){ %>
                                                <td class="text-center"> <div class="text-center badge badge-outline-primary"> <%= orderData[i].orderStatus %> </div></td>
                                            <% } else if (orderData[i].orderStatus === 'Process Completed' ){ %>
                                                <td class="text-center"> <div class="text-center badge badge-outline-success"> <%= orderData[i].orderStatus %> </div></td>
                                            <% } %>
                                        </tr>
                                    <% } %>
                                <% } else { %>
                                    <tr>
                                        <td colspan="11" class="text-danger"> <h5> No Items Found </h5></td>
                                    </tr>
                                <% } %>
                            </tbody>
                        </table>
                    </div>

                    <div class="list-pagination mt-4">
                        <nav>
                            <ul class="pagination rounded-flat pagination-success">
                                <!-- Previous Page Link -->
                                <% if (currentPage > 1) { %>
                                    <li class="page-item">
                                        <a class="page-link" href="?page=<%= currentPage - 1 %>&search=<%= searchQuery %>&sort=<%= sortQuery %>&order-status=<%= statusQuery %>&payment-method=<%= methodQuery %>">
                                            <i class="mdi mdi-chevron-left"></i>
                                        </a>
                                    </li>
                                <% } else { %>
                                    <li class="page-item disabled">
                                        <a class="page-link"><i class="mdi mdi-chevron-left"></i></a>
                                    </li>
                                <% } %>
                    
                                <!-- Dynamic Page Links -->
                                <% for(let j = 1; j <= totalPages; j++) { %>
                                    <% if (j === currentPage) { %>
                                        <li class="page-item active">
                                            <a class="page-link" href="#"><%= j %></a>
                                        </li>
                                    <% } else { %>
                                        <li class="page-item">
                                            <a class="page-link" href="?page=<%= j %>&search=<%= searchQuery %>&sort=<%= sortQuery %>&order-status=<%= statusQuery %>&payment-method=<%= methodQuery %>"><%= j %></a>
                                        </li>
                                    <% } %>
                                <% } %>
                    
                                <!-- Next Page Link -->
                                <% if (currentPage < totalPages) { %>
                                    <li class="page-item">
                                        <a class="page-link" href="?page=<%= currentPage + 1 %>&search=<%= searchQuery %>&sort=<%= sortQuery %>&order-status=<%= statusQuery %>&payment-method=<%= methodQuery %>">
                                            <i class="mdi mdi-chevron-right"></i>
                                        </a>
                                    </li>
                                <% } else { %>
                                    <li class="page-item disabled">
                                        <a class="page-link"><i class="mdi mdi-chevron-right"></i></a>
                                    </li>
                                <% } %>
                            </ul>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
